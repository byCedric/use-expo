{
	"expo": {
		"name": "expo hooks",
		"description": "complementary hooks for expo",
		"version": "0.0.0",
		"slug": "use-expo",
		"privacy": "public",
		"githubUrl": "https://github.com/bycedric/use-expo",
		"platforms": [
			"ios",
			"android",
			"web"
		],
		"icon": "./src/assets/images/app-icon.png",
		"splash": {
			"image": "./src/assets/images/app-splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": [
			"src/assets/**/*"
		],
		"ios": {
			"supportsTablet": true
		},
		"packagerOpts": {
			"config": "metro.config.js"
		}
	}
}
